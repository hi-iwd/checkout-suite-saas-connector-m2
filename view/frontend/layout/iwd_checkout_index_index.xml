<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      layout="checkout"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="checkout_index_index"/>
    <body>
        <referenceBlock name="page.main.title" remove="true"/>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="checkout.top.link" template="IWD_CheckoutConnector::link.phtml" after="-"/>
        </referenceContainer>
        <referenceBlock name="checkout.root">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">IWD_CheckoutConnector::frame.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>